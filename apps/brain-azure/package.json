{
  "name": "@simple-claude-bot/brain-azure",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/entry/*.js",
  "files": [
    "dist",
    "host.json",
    "node_modules"
  ],
  "scripts": {
    "build": "tsx build.ts",
    "type-check": "tsc -p tsconfig.check.json",
    "start": "func start",
    "package": "pnpm i --prod --force && zip publish.zip -FSr dist host.json package.json node_modules && pnpm i --force",
    "deploy:dev": "az functionapp deployment source config-zip --src publish.zip --resource-group sgh-rg-aue-dev-banananet-01 --name sgh-func-aue-dev-banananet-01 --build-remote false"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.41",
    "@anthropic-ai/claude-code": "^2.1.41"
  },
  "devDependencies": {
    "@azure/functions": "^4.11.2",
    "@shellicar/build-clean": "^1.2.2",
    "@shellicar/build-version": "^1.3.3",
    "@simple-claude-bot/brain-core": "workspace:*",
    "@simple-claude-bot/shared": "workspace:*",
    "@tsconfig/node22": "^22.0.5",
    "esbuild": "^0.27.3",
    "glob": "^13.0.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "zod": "^4.3.6"
  }
}
